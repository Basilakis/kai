{
  "name": "@kai/agents",
  "version": "0.1.0",
  "description": "CrewAI agent integration for KAI platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "setup": "node ./scripts/setup.js",
    "setup:unix": "chmod +x ./setup.sh && ./setup.sh",
    "verify": "node ./scripts/verify-setup.js",
    "test:integration": "node ./scripts/test-integration.js",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.1.1",
    "axios": "^1.6.0",
    "crewai": "^1.7.0",
    "crewai-tools": "^1.0.0",
    "dotenv": "^16.3.1",
    "langchain": "^0.1.0",
    "python-shell": "^5.0.0",
    "redis": "^4.6.0",
    "three": "^0.160.0",
    "winston": "^3.10.0",
    "@kai/shared": "workspace:*",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/uuid": "^9.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/python-shell": "^1.0.3",
    "@types/redis": "^4.0.11",
    "@types/three": "^0.160.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
