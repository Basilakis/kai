{
  "version": 2,
  "name": "kai-client",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "GATSBY_API_URL": "@kai_api_url",
    "GATSBY_SUPABASE_URL": "@kai_supabase_url",
    "GATSBY_SUPABASE_ANON_KEY": "@kai_supabase_anon_key"
  },
  "build": {
    "env": {
      "GATSBY_API_URL": "@kai_api_url",
      "GATSBY_SUPABASE_URL": "@kai_supabase_url",
      "GATSBY_SUPABASE_ANON_KEY": "@kai_supabase_anon_key"
    }
  }
}